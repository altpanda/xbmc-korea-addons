<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.baykoreans.net"
       name="BayKoreans"
       version="1.4.8"
       provider-name="XBMC-Korea">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.xbmcswift2" version="2.4.0"/>
    <import addon="script.module.beautifulsoup4" version="4.3.2"/>
    <import addon="script.module.youtube.dl" version="14.519.0" />
    <import addon="plugin.video.dailymotion_com" version="2.1.3" optional="true" />
  </requires>
  <extension point="xbmc.python.pluginsource"
             library="addon.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">BayKoreans Streaming Service</summary>
    <summary lang="ko">BayKoreans 비디오 스트리밍 서비스</summary>
    <description lang="en">Watch Korean TV shows in baykoreans.net</description>
    <description lang="ko">BayKoreans에서 제공하는 한국 TV프로를 볼 수 있습니다</description>
    <platform>all</platform>
  </extension>
</addon>
